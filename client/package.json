{
  "name": "@monopoly-js/client",
  "version": "0.1.0",
  "license": "WTFPL",
  "repository": {
    "type": "git",
    "url": "https://github.com/francois-roseberry/monopoly-js.git"
  },
  "scripts": {
    "lint:js": "jshint --verbose Gruntfile.js src/**/*.js test/**/*.js",
    "lint:css": "csslint src/**/*.css",
    "lint": "yarn lint:js && yarn lint:css",
    "build": "./build.sh",
    "test": "mocha -r module-alias/register -r jsdom-global/register -r ./mocha_inject test/**/*.test.js"
  },
  "devDependencies": {
    "bootstrap": "3.3.5",
    "browserify": "17.0.1",
    "browserify-transform-tools": "1.7.0",
    "csslint": "1.0.5",
    "expect.js": "0.3.1",
    "grunt": "1.6.1",
    "grunt-contrib-concat": "2.1.0",
    "grunt-contrib-cssmin": "5.0.0",
    "jquery": "2.1.4",
    "jquery-ui": "1.13.3",
    "jsdom": "25.0.1",
    "jsdom-global": "3.0.2",
    "jshint": "2.13.6",
    "mocha": "10.8.2",
    "module-alias": "2.2.3"
  },
  "dependencies": {
    "d3": "3.5.17",
    "rx": "2.5.3",
    "underscore": "1.8.3"
  },
  "packageManager": "yarn@4.5.1",
  "_moduleAliases": {
    "@test": "test",
    "@app": "src/application",
    "@domain": "src/domain",
    "@ui": "src/ui",
    "@infrastructure": "src/infrastructure",
    "@i18n": "src/infrastructure/i18n"
  }
}
